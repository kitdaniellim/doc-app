{
  "expo": {
    "name": "SetMeApp",
    "icon": "./assets/app_logo.png",
    "description": "Appointment application for professionals.",
    "slug": "SetMeApp",
    "sdkVersion": "37.0.0",
    "notification": {
      "icon": "./assets/app_logo.png"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash-setmeapp.png",
      "resizeMode": "contain",
      "backgroundColor": "#19BAB9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesIcloudStorage": true,
      "bundleIdentifier": "com.appointmentapp"
    },
    "android": {
      "package": "com.appointmentapp"
    }
  }
}